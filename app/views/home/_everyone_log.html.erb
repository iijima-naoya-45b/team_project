<div class = "row mx-auto border border-2 rounded-3 p-2 mb-2 col-lg-11">
  <div class ="accordion col-sm-2 col-md-1 col-lg-3 border-end">
    <%= log[:profile_name] %>は
    <%= log[:log][:hour] %>時間
    <%= log[:log][:minutes] %>分勉強したよ！
  </div>
  <div class ="col-sm-10 col-md-2  col-lg-2 border-end truncate">
    <%= log[:log][:title] %>
  </div>
  <div class ="col-sm-9 col-md-7 col-lg-7 truncate">
    <%= log[:log][:body] %>
  </div>
</div>

